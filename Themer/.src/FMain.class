  Shell "gsettings set org.gnome.desktop.interface gtk-theme 'Mint-X'"
  Shell "gsettings set org.gnome.desktop.wm.preferences theme 'Mint-X'"
  Shell "gsettings set org.gnome.desktop.interface icon-theme 'Mint-X'"
  Shell "gsettings set org.cinnamon.theme name 'Linux Mint'"
  Shell "gsettings set org.cinnamon.desktop.wm.preferences theme 'Mint-X'"
Shell "gsettings set org.cinnamon.desktop.interface gtk-theme 'Mint-X'"
Shell "gsettings set org.cinnamon.desktop.interface icon-theme 'Mint-X'"
Shell "gsettings set org.gnome.shell.overrides button-layout :minimize,maximize,close"
